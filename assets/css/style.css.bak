html {
	background-image: url(https://wallpaper-mania.com/wp-content/uploads/2018/09/High_resolution_wallpaper_background_ID_77700409780.jpg);
	font-size: 15px;
	width: 100vw;
	font-family: "Georgia";
	}
	
h1 {
	font-size: 4em;
	font-family: "Papyrus";
	}

h2 {
	font-size: 2em;
	font-family: "Papyrus";
	}

#page {
	margin: auto;
	text-align: center;
	width: 70%;
	background-image: linear-gradient(LightPink, LightSalmon, LemonChiffon, Thistle, Plum);
	border: 5px solid RosyBrown;
	}	

.entete {text-align: center;}

#titre {
		font-size: 1.5em;
		font-weight: bold;
		font-family: "Papyrus";
		background-color: Thistle;
		text-align: center;
		}

#aboutme {
	margin: 30px;
	padding: 50px;
	font-size: 1.3em;
	color: black;
	text-align: left;
	background-image: linear-gradient(RosyBrown, PaleVioletRed);
	border: 2px solid salmon
	box-shadow: 6px 6px 0px rgba(0, 0, 0);
	}

#tableau {
	background-image: linear-gradient(LightPink, LightSalmon, LemonChiffon, Plum);
	}

.footer {text-align: center;}
